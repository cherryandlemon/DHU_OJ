#include <stdio.h>
#include<stdlib.h>
#define maxn 3005
int pro57()
{
    int n;
    while(scanf("%d",&n)!=EOF)
    {
        int res[maxn];
        memset(res,0,sizeof(res));
        int cnt=1;
        res[0]=1;
        if(n==1)
            printf("%d\n",1);
        else
        {
            for(int i=2;i<=n;i++)
            {
                int add=0;
                for(int j=0;j<maxn;j++)
                {
                    int s =res[j]*i+add;
                    res[j]=s%10;
                    add=s/10;
                }
            }
            int t;
            for(t=maxn-1;t>=0;t--)
              if(res[t])break;
            for(int i=t;i>=0;i--)
                printf("%d",res[i]);
            printf("\n");
        }
    }
}
int main()
{
	//freopen("E:\\2_rub\\donhuakaoyanzhuanyeke\\DongHuaOj\\DongHuaOj\\in.txt", "r", stdin);
	//freopen("E:\\2_rub\\donhuakaoyanzhuanyeke\\DongHuaOj\\DongHuaOj\\out.txt","w",stdout);
	pro57();
	return 0;
}
