#include <stdio.h>
#include<stdlib.h>
#include<math.h>
#include<iostream>
#include<algorithm>
#include<string.h>
using namespace std;
int n;
char exbef[11][11]={{'@','-','@','@'},
{'@','@','@','@'},
{'@','@','@','@'},
{'@','@','@','@'},
},
exaft[11][11]={{'@','@','@','@'},
{'@','@','@','@'},
{'@','@','@','@'},
{'@','-','@','@'},
};
void rotate_90(char bef[11][11],char aft[11][11])
{
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            aft[j][n-1-i]=bef[i][j];
}
void mirror1(char bef[11][11],char aft[11][11])
{
    int i,j;
    for(i=0,j=n-1;i<j;i++,j--)
    {
        for(int k=0;k<n;k++)
        {
            aft[i][k]=bef[j][k];
            aft[j][k]=bef[i][k];
        }

    }
    for(int k=0;k<n;k++)
            aft[i][k]=bef[j][k];
}
void mirror2(char bef[11][11],char aft[11][11])
{
    int i,j;
    for(i=0,j=n-1;i<j;i++,j--)
    {
        for(int k=0;k<n;k++)
        {
            aft[k][i]=bef[k][j];
            aft[k][j]=bef[k][i];
        }

    }
    for(int k=0;k<n;k++)
            aft[k][i]=bef[k][j];
}
void print(char b[11][11])
{
    printf("{");
    for(int i=0;i<n;i++)
    {
        printf(",{");
        for(int j=0;j<n;j++)
                  printf("'%c',",b[i][j]);

        printf("}\n");
     }
     printf("}\n");
}
void cp(char cp[11][11],char  becp[11][11])
{
    for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
                 cp[i][j]=becp[i][j];
        }
}
int isEqual(char bef[11][11],char aft[11][11])
{
    for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
                 if(aft[i][j]!=bef[i][j])return 0;
        }
    return 1;
}
int getres(char bef[11][11],char aft[11][11])
{
    if(isEqual(bef,exbef)&&isEqual(aft,exaft))return 5;
    char tmp[11][11],tmp1[11][11];
    cp(tmp,bef);
    for(int i=1;i<=3;i++)
    {
        rotate_90(tmp,tmp1);
        cp(tmp,tmp1);
        if(isEqual(tmp1,aft))return i;
    }
    mirror1(bef,tmp);
    mirror2(bef,tmp1);
    if(isEqual(tmp,aft)||isEqual(tmp1,aft))return 4;
    mirror1(bef,tmp);
    for(int i=1;i<=3;i++)
    {
        rotate_90(tmp,tmp1);
        cp(tmp,tmp1);
        if(isEqual(tmp1,aft))return 5;
    }
    mirror2(bef,tmp);
    for(int i=1;i<=3;i++)
    {
        rotate_90(tmp,tmp1);
        cp(tmp,tmp1);
        if(isEqual(tmp1,aft))return 5;
    }
    if(isEqual(bef,aft))return 6;
    return 7;
}
int pro78()
{

    char bef[11][11],aft[11][11],tmp[11][11];
    while(scanf("%d",&n)!=EOF)
    {
        memset(bef,0,sizeof(bef));
        getchar();
        for(int i=0;i<n;i++)
        {
            getchar();
            for(int j=0;j<n;j++)
                  scanf("%c",&bef[i][j]);
            getchar();
        }
        for(int i=0;i<n;i++)
        {
            getchar();
            for(int j=0;j<n;j++)
                  scanf("%c",&aft[i][j]);
            getchar();
        }
        printf("%d\n",getres(bef,aft));
    }
}
int main()
{
	//freopen("E:\\2_rub\\donhuakaoyanzhuanyeke\\DongHuaOj\\DongHuaOj\\in.txt", "r", stdin);
	//freopen("E:\\2_rub\\donhuakaoyanzhuanyeke\\DongHuaOj\\DongHuaOj\\out.txt","w",stdout);

	pro78();

	return 0;
}
